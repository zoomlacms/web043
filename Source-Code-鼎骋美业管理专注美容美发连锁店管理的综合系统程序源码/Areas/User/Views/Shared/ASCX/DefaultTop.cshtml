@functions{
    string usernavBan = ZoomLa.Components.SiteConfig.UserConfig.UserNavBan;
    public bool IsShowNav(string nav)
    {
        if (string.IsNullOrEmpty(usernavBan)) { return true; }
        else { return !usernavBan.Contains(nav); }
    }
